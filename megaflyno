-- Хук на активацию Xeno
callbacks.Register("Draw", "CheaterAnnouncement", function()
    if input.IsButtonDown(KEY_INSERT) then -- INSERT - стандартная клавиша активации Xeno
        OnActivate()
        -- Удаляем хук после первого срабатывания, чтобы не спамить
        callbacks.Unregister("Draw", "CheaterAnnouncement")
    end
end)
